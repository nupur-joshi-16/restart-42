// Data types in Rust
pub fn run() {
    // Basic Data Types
    // ------------------------ //
    // Boolean - true, false
    // Integer - 1, 2, 50, 99, -2
    // Double-Float - 1.1, 5.5, 200.0001, 2.0
    // Character - 'A', 'B', 'C', '6', '$'
    // String - "Hello", "string", "this is a string", "restart-42"
}